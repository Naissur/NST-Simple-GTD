<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="@color/SimpleGTD.Todo.pressed_color" />
      <stroke
          android:width="1px"
          android:color="@color/SimpleGTD.Todo.border_color" />
    </shape>
  </item>
  <item>    <!-- Render by default -->
    <shape>
      <solid
          android:color="@color/SimpleGTD.Todo.color" />
      <stroke
          android:width="1px"
          android:color="@color/SimpleGTD.Todo.border_color" />
    </shape>
  </item>
</selector>