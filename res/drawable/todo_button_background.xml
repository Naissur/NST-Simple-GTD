<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="@color/SimpleGTD.todo_pressed_color" />
      <stroke
          android:width="1dp"
          android:color="@color/SimpleGTD.todo_border_color" />
    </shape>
  </item>
  <item>    <!-- Render by default -->
    <shape>
      <solid
          android:color="@color/SimpleGTD.todo_unpressed_color" />
      <stroke
          android:width="1dp"
          android:color="@color/SimpleGTD.todo_border_color" />
    </shape>
  </item>
</selector>